<h2>Links</h2>
<ul>
<%
    link_keys.each do |link_key|
        linkObj = @links[link_key.to_sym] 
        %> <li>
        <%= link_to linkObj.text, linkObj.url  %> 
        <% if linkObj.remote %>
            <%= image_tag('wikipediaExternalPageGold.png', border:'0', alt:"External Page")%> 
        <% end %>
        </li> 
    <% end
%>    
</ul>

